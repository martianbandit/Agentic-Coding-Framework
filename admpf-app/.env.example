# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORGANIZATION=your_organization_id_optional

# Default AI Models (matching user preferences)
DEFAULT_TEXT_MODEL=gpt-4o
DEFAULT_IMAGE_MODEL=dall-e-3

# Default Parameters
DEFAULT_TEMPERATURE=0.7
DEFAULT_MAX_TOKENS=2000
DEFAULT_TOP_P=1
DEFAULT_FREQUENCY_PENALTY=0
DEFAULT_PRESENCE_PENALTY=0
DEFAULT_IMAGE_RESOLUTION=1024x1024
DEFAULT_IMAGE_STYLE=natural
DEFAULT_IMAGE_QUALITY=standard

# Model Memory Configuration
# Duration to store successful predictions (in seconds)
MODEL_MEMORY_STORAGE_DURATION=3600
# Number of recent inputs to store per user
USER_INPUTS_HISTORY_COUNT=3
# Whether to sync default parameters to user profile
STORE_DEFAULT_PARAMETERS=true

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_key_here

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
